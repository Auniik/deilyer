{
    "name": "auniik/deilyer",
    "description": "Its a skeleton app to start with",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "bootstrap/core/"
        },
        "files": [
            "bootstrap/core/Helpers/functions.php"
        ]
    },
    "authors": [
        {
            "name": "Anik Datta"
        }
    ],
    "require-dev": {
        "symfony/var-dumper": "^6.2",
        "composer/composer": "^2.7"
    },
    "require": {
        "vlucas/phpdotenv": "^5.5",
        "ext-pdo": "*",
        "delight-im/auth": "^8.3"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
